# Aye-Aye Chef Environment Configuration
# Copy this file to .env and update with your AWS configuration

# AWS Cognito Configuration
EXPO_PUBLIC_USER_POOL_ID=us-west-2_xxxxxxxxx
EXPO_PUBLIC_USER_POOL_CLIENT_ID=xxxxxxxxxxxxxxxxxxxxxxxxxx

# API Gateway Configuration  
EXPO_PUBLIC_API_URL=https://xxxxxxxxxx.execute-api.us-west-2.amazonaws.com/prod/

# S3 Bucket Configuration
EXPO_PUBLIC_IMAGES_BUCKET=aye-aye-images-xxxxxxxxxxxx-xxxxxxxxxxxx

# AWS Region
EXPO_PUBLIC_AWS_REGION=us-west-2

# Optional: Development Settings
# EXPO_PUBLIC_DEBUG_MODE=true
# EXPO_PUBLIC_LOG_LEVEL=debug