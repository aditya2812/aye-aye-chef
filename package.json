{
  "name": "aye-aye-chef",
  "version": "1.0.0",
  "description": "AI-powered recipe generation system that transforms ingredients into personalized recipes using computer vision and AWS Bedrock",
  "keywords": ["ai", "recipe", "cooking", "computer-vision", "aws", "react-native", "expo", "food"],
  "homepage": "https://aye-aye-chef.netlify.app",
  "repository": {
    "type": "git",
    "url": "https://github.com/aditya2812/aye-aye-chef.git"
  },
  "license": "MIT",
  "author": "Aye-Aye Chef Team",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "test:lambda": "cd tests && npm test",
    "test:integration": "cd tests && npm run integration",
    "cdk": "cdk",
    "deploy": "cdk deploy --all",
    "destroy": "cdk destroy --all",
    "setup": "npm install && cd mobile-app && npm install",
    "dev": "cd mobile-app && npm run web",
    "build:web": "cd mobile-app && npm run build:web",
    "deploy:web": "cd mobile-app && npm run deploy:netlify"
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@types/node": "20.1.0",
    "@types/react": "~19.1.10",
    "aws-cdk": "2.160.0",
    "jest": "^29.4.0",
    "ts-jest": "^29.1.0",
    "typescript": "~5.1.3"
  },
  "dependencies": {
    "@aws-sdk/client-rds-data": "^3.911.0",
    "aws-cdk-lib": "2.160.0",
    "constructs": "^10.0.0",
    "expo": "^54.0.13"
  }
}
